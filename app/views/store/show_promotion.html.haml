#store-wrap
  #store-header
    #categories-wrap
      .categories.hidden-print
        %ul= render @categories

  #store-body
    #products-wrap
      .products
        .page-header.visible-print
          %h1= @promotion.to_s
        #product-view.product-grid
          %ul{class: current_store.masonry? && 'masonry-grid'}
            = render @products

.table-responsive
  %table.table.table-striped.table-hover.table-loose
    %caption
      %span= col(Product, :images)
    %thead
      %tr
        %th.col-xs-8= title(Image)
        %th.col-xs-4= col(Image, :image_type_id)

    %tbody
      = render partial: 'admin/images/row', collection: imageable.images.ordered, as: :image
